{
  "name": "react-webpack-bootstrap",
  "version": "1.0.2",
  "description": "React / webpack boilerplate with babel (es6) transpiler",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.jsx",
    "build": "NODE_ENV=production webpack -p --config ./webpack/production.config.js",
    "start": "nodemon -e js,jsx --watch app.js --watch server.js server.js --exec babel-node",
    "start-production": "NODE_ENV=production babel-node server.js"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "Jan-Erik Br√•then",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "domready": "^1.0.8",
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janerikbr/react-webpack-bootstrap"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "babel-loader": "^6.2.2",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "nodemon": "^1.8.1",
    "postcss": "^5.0.15",
    "postcss-loader": "^0.8.0",
    "pre-commit": "^1.1.2",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
